{
    "manifest_version": 2,
    "name": "Search with me",
    "version": "0.0.1",
    "content_scripts":[
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Lookup a definition"
    }
}